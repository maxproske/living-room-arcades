{
    "extends": ["react-app", "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "bracketSpacing": true,
                "arrowParens": "always",
                "printWidth": 80,
                "semi": false,
                "trailingComma": "es5",
                "tabWidth": 2,
                "useTabs": false
            }
        ]
    }
}
  